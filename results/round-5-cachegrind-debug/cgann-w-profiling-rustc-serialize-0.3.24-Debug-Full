        .                           return id;
        .                       }
        .                       let next = hir.get_parent_node(id);
  726,147 ( 0.01%)              if next == id {
        .                           bug!("lint traversal reached the root of the crate");
        .                       }
        .                       id = next;
        .                   }
  617,900 ( 0.01%)      }
