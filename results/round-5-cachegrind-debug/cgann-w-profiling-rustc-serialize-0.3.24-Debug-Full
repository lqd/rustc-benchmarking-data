         .                   }
         .           
         .                   // There's no need to perform marking, cycle processing and compression when nothing
         .                   // changed.
    68,731 ( 0.00%)          if !outcome.is_stalled() {
         .                       self.mark_successes();
         .                       self.process_cycles(processor);
    51,238 ( 0.00%)              self.compress(|obl| outcome.record_completed(obl));
         .                   }
