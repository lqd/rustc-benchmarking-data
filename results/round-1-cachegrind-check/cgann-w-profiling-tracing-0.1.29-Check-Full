      .           /// - Err(span) if the next token tree is not a token
  8,885 ( 0.00%)  fn parse_kleene_op(
      .               input: &mut impl Iterator<Item = tokenstream::TokenTree>,
      .               span: Span,
      .           ) -> Result<Result<(KleeneOp, Span), Token>, Span> {
