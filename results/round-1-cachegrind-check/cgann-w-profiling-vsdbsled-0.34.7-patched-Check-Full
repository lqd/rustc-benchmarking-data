  494,568 ( 0.01%)      fn compress(&mut self, mut outcome_cb: impl FnMut(&O)) {
   66,822 ( 0.00%)          let orig_nodes_len = self.nodes.len();
        .                   let mut node_rewrites: Vec<_> = std::mem::take(&mut self.reused_node_vec);
        .                   debug_assert!(node_rewrites.is_empty());
