        .                   const TAG_INVALID_SPAN: u8 = 1;
        .                   const TAG_RELATIVE_SPAN: u8 = 2;
        .           
  409,034 ( 0.00%)          if !ctx.hash_spans() {
        .                       return;
        .                   }
        .           
1,946,284 ( 0.02%)          let span = self.data_untracked();
  791,774 ( 0.01%)          span.ctxt.hash_stable(ctx, hasher);
1,009,422 ( 0.01%)          span.parent.hash_stable(ctx, hasher);
        .           
  677,560 ( 0.01%)          if span.is_dummy() {
        .                       Hash::hash(&TAG_INVALID_SPAN, hasher);
        .                       return;
        .                   }
        .           
  663,724 ( 0.01%)          if let Some(parent) = span.parent {
        .                       let def_span = ctx.def_span(parent).data_untracked();
        .                       if def_span.contains(span) {
        .                           // This span is enclosed in a definition: only hash the relative position.
        .                           Hash::hash(&TAG_RELATIVE_SPAN, hasher);
        .                           (span.lo - def_span.lo).to_u32().hash_stable(ctx, hasher);
        .                           (span.hi - def_span.lo).to_u32().hash_stable(ctx, hasher);
