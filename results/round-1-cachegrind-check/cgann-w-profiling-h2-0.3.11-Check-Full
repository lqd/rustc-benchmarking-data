        .               fn check_for_redundant_imports(
        .                   &mut self,
        .                   ident: Ident,
        .                   import: &'b Import<'b>,
        .                   source_bindings: &PerNS<Cell<Result<&'b NameBinding<'b>, Determinacy>>>,
        .                   target_bindings: &PerNS<Cell<Option<&'b NameBinding<'b>>>>,
        .                   target: Ident,
        .               ) {
        .                   // Skip if the import was produced by a macro.
    1,572 ( 0.00%)          if import.parent_scope.expansion != LocalExpnId::ROOT {
        .                       return;
        .                   }
        .           
        .                   // Skip if we are inside a named module (in contrast to an anonymous
        .                   // module defined by a block).
    1,671 ( 0.00%)          if let ModuleKind::Def(..) = import.parent_scope.module.kind {
        .                       return;
        .                   }
        .           
       24 ( 0.00%)          let mut is_redundant = PerNS { value_ns: None, type_ns: None, macro_ns: None };
        .           
       48 ( 0.00%)          let mut redundant_span = PerNS { value_ns: None, type_ns: None, macro_ns: None };
        .           
      528 ( 0.00%)          self.r.per_ns(|this, ns| {
      132 ( 0.00%)              if let Ok(binding) = source_bindings[ns].get() {
       60 ( 0.00%)                  if binding.res() == Res::Err {
        .                               return;
        .                           }
        .           
        .                           let orig_unusable_binding =
       24 ( 0.00%)                      mem::replace(&mut this.unusable_binding, target_bindings[ns].get());
        .           
      144 ( 0.00%)                  match this.early_resolve_ident_in_lexical_scope(
       36 ( 0.00%)                      target,
        .                               ScopeSet::All(ns, false),
       24 ( 0.00%)                      &import.parent_scope,
        .                               false,
        .                               false,
        .                               import.span,
        .                           ) {
        .                               Ok(other_binding) => {
        .                                   is_redundant[ns] = Some(
        .                                       binding.res() == other_binding.res() && !other_binding.is_ambiguity(),
        .                                   );
        .                                   redundant_span[ns] = Some((other_binding.span, other_binding.is_import()));
        .                               }
       24 ( 0.00%)                      Err(_) => is_redundant[ns] = Some(false),
        .                           }
        .           
       24 ( 0.00%)                  this.unusable_binding = orig_unusable_binding;
        .                       }
      288 ( 0.00%)          });
        .           
