        .                   cmp::min(
    9,351 ( 0.00%)              if let Some(def_id) = def_id.as_local() {
    3,492 ( 0.00%)                  find.access_levels.map.get(&def_id).copied()
        .                       } else {
        .                           Self::MAX
        .                       },
    4,971 ( 0.00%)              find.min,
        .                   )
   12,468 ( 0.00%)      }
        .           }
