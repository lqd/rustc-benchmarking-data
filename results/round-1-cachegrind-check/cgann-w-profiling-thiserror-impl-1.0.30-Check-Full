      .               mut handle_inactive_variant: impl FnMut(MovePathIndex),
      .           ) {
  1,780 ( 0.00%)      let enum_mpi = match move_data.rev_lookup.find(enum_place.as_ref()) {
      .                   LookupResult::Exact(mpi) => mpi,
      .                   LookupResult::Parent(_) => return,
      .               };
      .           
      .               let enum_path = &move_data.move_paths[enum_mpi];
  1,398 ( 0.00%)      for (variant_mpi, variant_path) in enum_path.children(&move_data.move_paths) {
      .                   // Because of the way we build the `MoveData` tree, each child should have exactly one more
      .                   // projection than `enum_place`. This additional projection must be a downcast since the
      .                   // base is an enum.
    212 ( 0.00%)          let (downcast, base_proj) = variant_path.place.projection.split_last().unwrap();
    848 ( 0.00%)          assert_eq!(enum_place.projection.len(), base_proj.len());
      .           
    636 ( 0.00%)          let variant_idx = match *downcast {
      .                       mir::ProjectionElem::Downcast(_, idx) => idx,
      .                       _ => unreachable!(),
      .                   };
      .           
    212 ( 0.00%)          if variant_idx != active_variant {
      .                       on_all_children_bits(tcx, body, move_data, variant_mpi, |mpi| {
    206 ( 0.00%)                  handle_inactive_variant(mpi)
      .                       });
      .                   }
      .               }
